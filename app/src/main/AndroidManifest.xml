<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/baseline_30fps"
        android:label="@string/app_name"
        android:roundIcon="@drawable/baseline_30fps"
        android:supportsRtl="true"
        android:theme="@style/Theme.DynamicAppIcon"
        tools:targetApi="31">
        <activity
            android:name="${main_activity}"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.DynamicAppIcon">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity-alias
            android:icon="@drawable/baseline_60fps"
            android:roundIcon="@drawable/baseline_60fps"
            android:name="${main_activity_alias_zero}"
            android:targetActivity=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>

        <activity-alias
            android:icon="@drawable/baseline_cloud_circle_24"
            android:roundIcon="@drawable/baseline_cloud_circle_24"
            android:name="${main_activity_alias_one}"
            android:targetActivity=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>

    </application>

</manifest>
